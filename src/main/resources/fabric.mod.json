		"client": [],
		"main": [
			"com.zensonaton.minecraftclientgobrrr.MinecraftClientGobrrr"
		],
		"preLaunch": [
			"com.zensonaton.minecraftclientgobrrr.MinecraftClientGobrrrPreLaunchEntrypoint"
		]
	},
	"accessWidener": "minecraftclientgobrrr.accesswidener",
