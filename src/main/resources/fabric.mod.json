{
	"schemaVersion": 1,
	"id": "minecraftclientgobrrr",
	"version": "${version}",
	"name": "MinecraftClient go brrr",
	"description": "Lightweight client-side mod that makes changes to the MinecraftClient class, resulting in a smoother loading experience.",
	"authors": [
		"Zensonaton"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/minecraftclientgobrrr",
		"repo": "https://github.com/Zensonaton/MinecraftClientGobrrr",
		"issues": "https://github.com/Zensonaton/MinecraftClientGobrrr/issues",
		"sources": "https://github.com/Zensonaton/MinecraftClientGobrrr"
	},
	"license": "MIT",
	"icon": "assets/MinecraftClientGobrrr/icon.png",
